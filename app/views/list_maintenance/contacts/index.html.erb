<%= render_component('page') do %>
  <%= render_component('index-header', { resource: 'Contact', path:  new_contact_path }) %>

  <%= render_component(
    'index-table',
    { resources: @contacts,
      attributes: [
        {  name: :name },
        {  email: :email },
        {  organization: :organization_label },
        {  state:  :state_code }
      ]
    }
  )%>
<% end %>
