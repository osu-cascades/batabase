<%= render_component('page') do %>
  <%= render_component('index-header', { resource: 'Sample Design', path:  new_sample_design_path }) %>

  <%= render_component(
    'index-table',
    { resources: @sample_designs,
      attributes: [
        { label: :label },
        { type: :sample_design_type_label },
        { description: :description },
      ]
    }
  )%>
<% end %>