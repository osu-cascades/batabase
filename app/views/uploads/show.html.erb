<div class="container main-container">
    <div class="table-responsive">
      <table class="table smaller-font">
        <thead class="thead-dark">
          <tr>
            <% @upload_csv[0].each do |header| %>
              <th scope="col"><%= header %></th>
            <% end %>
          </tr>
        </thead>
        <tbody>
          <% @upload_csv[1..-1].each do |row| %>
            <tr>
              <% row.each do |col| %>
                <td><%= col %></td>
              <% end %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>    
</div>